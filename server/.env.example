PORT=9999
MONGODB_URI=mongodb://localhost:27017/test
JWT_SECRET=your_jwt_secret_key_here
NODE_ENV=development
UPLOAD_DIR=uploads

# Logging Configuration
# LOG_LEVEL: ERROR (только ошибки) | WARN (предупреждения и ошибки) | INFO (стандартный) | DEBUG (все логи)
LOG_LEVEL=INFO

# Groq API for AI-powered test parsing
# Можно добавить несколько ключей через запятую
# Система автоматически переключится на следующий при достижении лимита
GROQ_API_KEY=gsk_key1,gsk_key2,gsk_key3

# Redis Configuration (optional, for caching)
# Set REDIS_ENABLED=true to enable Redis caching
REDIS_ENABLED=false
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=your_redis_password_here

# AWS S3 Configuration (for file storage)
# Use MinIO for local development (self-hosted S3-compatible storage)
AWS_REGION=us-east-1
AWS_ACCESS_KEY=your_access_key_here
AWS_SECRET_KEY=your_secret_key_here
S3_BUCKET=resultma-exports
S3_ENDPOINT=http://localhost:9000  # MinIO endpoint for local dev
USE_MINIO=true  # Set to false for production AWS S3

# BullMQ Worker Configuration
WORKER_CONCURRENCY=10  # Number of parallel jobs per worker
WORKER_MAX_JOBS_PER_MINUTE=100  # Rate limiting

# CRM Integration (Math Academy CRM)
CRM_API_URL=https://crm.mathacademy.uz/api
CRM_API_KEY=sk_live_xxxxxxxxxxxxxxxxx
CRM_BEARER_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
CRM_SYNC_ENABLED=true
CRM_SYNC_INTERVAL=*/5 * * * *